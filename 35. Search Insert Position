# Search Insert Position
class Solution:
    def searchInsert(self, nums: List[int], target: int) -> int:

        low = 0
        high = len(nums)

        while low < high:
            mid = low + (high - low) // 2

            if nums[mid] < target:
                left = mid + 1
            
            else:
                right = high
        
        return low
